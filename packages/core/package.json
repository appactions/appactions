{
  "name": "@appactions/core",
  "version": "1.0.3",
  "description": "App Actions early preview",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --config cypress/app/webpack.config.js",
    "cypress": "node cypress.js",
    "test:cypress": "start-server-and-test http://localhost:12000 cypress",
    "test:jest": "jest",
    "test": "yarn test:cypress && yarn test:jest",
    "wait-for-dev": "yarn run clean && wait-on build/index.js && wait-on build/browser-extension/main.js",
    "dev": "concurrently \"yarn:dev:*\"",
    "dev:library": "NODE_ENV=development rollup --config rollup.config.js --watch",
    "dev:extension": "NODE_ENV=development webpack --config webpack.config.js --watch",
    "clean": "rm -rf build",
    "build": "yarn run clean && yarn run build:extension && yarn run build:library",
    "build:extension": "NODE_ENV=production webpack --config webpack.config.js",
    "build:library": "NODE_ENV=production rollup --config rollup.config.js",
    "build:react-devtools-renderer": "rollup --config src/vendor/rollup.config.js",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/appactions/appactions.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "Miklos Megyes <hello@miklos.dev>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.8.3",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "~18",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.0",
    "@webpack-cli/serve": "^1.6.1",
    "autoprefixer": "10.4.2",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "classnames": "^2.3.1",
    "clipboard-js": "^0.3.6",
    "copy-webpack-plugin": "^10.2.4",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^5.2.7",
    "cypress": "^4.6.0",
    "error-stack-parser": "^2.0.6",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "process": "^0.11.10",
    "react-error-boundary": "^3.1.4",
    "react-highlight": "^0.14.0",
    "react-router-dom": "^5.0.0",
    "rollup": "~2.54",
    "rollup-plugin-multi-input": "^1.3.1",
    "semver": "^7.1.3",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^3.0.23",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/unique-selector": "^0.4.2",
    "chokidar": "^3.5.3",
    "common-tags": "^1.8.2",
    "core-js": "3",
    "debug": "^4.3.3",
    "lodash.isequal": "^4.5.0",
    "lodash.ismatch": "^4.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "temp-write": "^4.0.0",
    "yaml": "^1.10.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/vendor/"
    ]
  }
}

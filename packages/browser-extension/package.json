{
    "version": "0.0.1",
    "name": "browser-extension",
    "scripts": {
        "clean": "rm -rf build",
        "build": "yarn run clean && NODE_ENV=production webpack --config webpack.config.js",
        "dev": "NODE_ENV=development webpack --config webpack.config.js --watch",
        "start": "start-server-and-test testapp http://localhost:12000 'cypress open'",
        "test": "start-server-and-test testapp http://localhost:12000 'cypress run'",
        "testapp": "webpack serve --config cypress/app/webpack.config.js"
    },
    "devDependencies": {
        "@ant-design/icons": "^4.7.0",
        "@babel/core": "^7.17.7",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@react-spring/web": "^9.3.2",
        "@svgr/webpack": "^4.3.3",
        "@webpack-cli/serve": "^1.6.0",
        "antd": "^4.18.2",
        "autoprefixer": "^9",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.0.4",
        "csp-html-webpack-plugin": "^5.1.0",
        "css-loader": "5.2.7",
        "cypress": "^9.2.0",
        "html-webpack-plugin": "^4.0.0-beta.8",
        "jotai": "^1.5.0",
        "postcss": "^7",
        "postcss-loader": "^4.2.0",
        "start-server-and-test": "^1.14.0",
        "style-loader": "2.0.0",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat",
        "webpack": "^4.41.2",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^3.10.3",
        "webpack-extension-reloader": "^1.1.1",
        "webpack-manifest-version-sync-plugin": "^0.0.3"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.4.0",
        "classnames": "^2.3.1",
        "clipboard-js": "^0.3.6",
        "core-js": "^3.3.3",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.3",
        "react-highlight": "^0.14.0",
        "regenerator-runtime": "^0.13.3",
        "webextension-polyfill": "^0.5.0"
    }
}
